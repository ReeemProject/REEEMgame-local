{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "commonjs",
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es2015",
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ],
  "paths": {
    "react": [ "node_modules/@types/react" ]
  }
}
